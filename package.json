{
  "name": "react-router-menu-example",
  "version": "0.1.0",
  "description": "testing out react-router-menu",
  "main": "index.js",
  "scripts": {
    "setup": "yarn ; cd server ; yarn",
    "start": "yarn build ; yarn server; google-chrome http://localhost:3001/",

    "build": "webpack -d --mode development  --progress",
    "server": "cd server ; npx babel-watch server.js 3001 &",

    "dev": "npm-run-all --parallel watch:*",
    "watch:hotload": "webpack-dev-server --mode development  --devtool eval-source --progress --colors  --hot --inline | grep -v 'built' ",
    "watch:start-api": "cd server; npx babel-watch server.js 8081 ",
    "watch:start-browser": "google-chrome http://localhost:8080/",

    "build:watch": "webpack -w -d --mode development  --progress",
    "pack": "webpack -p --mode production  --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-menu"
  ],
  "author": "Martin A. Jackson",
  "license": "ISC",
  "dependencies": {
    "@version/react-router-v3": "^1.0.0",
    "antd": "^3.2.1",
    "axios": "^0.18.0",
    "react": "^16.2.0",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.2.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015-webpack": "^6.4.3",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.9",
    "json-loader": "^0.5.7",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
