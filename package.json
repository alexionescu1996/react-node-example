{
  "name": "react-node-example",
  "version": "0.1.2",
  "description": "testing out react-router-menu",
  "main": "index.js",
  "scripts": {
    "setup": "yarn && cd server && yarn",
    "start": "npm-run-all --parallel start:*",
    "dev": "npm-run-all --parallel dev:*",
    "start:front-end": "webpack --mode development  --progress",
    "start:server": "cd server && npx babel-watch server.js 3001 ",
    "start:browser": "wait-on http://localhost:3001/ && opn http://localhost:3001/",
    "dev:build-front-end": "webpack-dev-server --mode development  --devtool eval-source --progress --colors  --hot --inline | grep -v 'built' ",
    "dev:build-back-end": "cd server && npx babel-watch server.js 8081 ",
    "pack": "webpack -p --mode production  --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-menu",
    "node",
    "express"
  ],
  "author": "Martin A. Jackson <martin.a.jackson@gmail.com> (http://martinjackson.github.io)",
  "homepage": "https://github.com/martinjackson/react-node-example#readme",
  "bugs": {
    "url": "https://github.com/martinjackson/react-node-example/issues",
    "email": "martin.a.jackson@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@version/react-router-v3": "^1.0.0",
    "antd": "^3.7.0",
    "axios": "^0.18.0",
    "immutable": "^3.8.2",
    "madge": "^3.2.0",
    "react": "^16.4.1",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.4.1",
    "react-flexbox-grid": "^2.1.2",
    "react-markdown": "^3.3.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-webpack": "^6.4.3",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^3.1.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "style-loader": "^0.21.0",
    "typescript": "^2.9.2",
    "wait-on": "^2.1.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  }
}
